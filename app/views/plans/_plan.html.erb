<tr>
  <td>
    <%= link_to plan.name, country_provider_plan_url(@country, @provider, plan) %>
  </td>
  <%if session[:user_id]%>
    <td>
      <span style="color:#ffa500; font-weight:bold;font-size: 14px;">&#916;</span> <%= link_to "Edit", edit_country_provider_plan_url(@country, @provider, plan) %>
    </td>
    <td>
      <span style="color:#ff0000; font-weight:bold;font-size: 16px;">&#215;</span> <%= link_to "Delete", country_provider_plan_url(@country, @provider, plan), :confirm => "Are you sure you want to delete this plan", :method => :delete %>
    </td>
  <%end%>
</tr>
